<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Gallery" table="gallery">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nameActivity" type="string" column="name_activity" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="urlImage" type="string" column="url_image" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="altImage" type="string" column="alt_image" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
