<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Employed" table="employed">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstName" type="string" column="first_name" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="position" type="string" column="position" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="function" type="string" column="function" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="experience" type="integer" column="experience" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="urlImage" type="string" column="url_image" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
