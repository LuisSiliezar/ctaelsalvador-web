{% spaceless %}
<!DOCTYPE html>
<html lang="es">
<head>
<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="author" content="Víctor M. Reyes | vreyes@vkings.net" />
<meta name="theme-color" content="#0A3344">
<meta name="image" content="{{ asset('images/ctaelsalvador-logo.png') }}" />
<meta name="Description" content="Somos una institución especializada en brindar una salida a las personas que tienen problemas con el abuso de sustancias y dar apoyo a sus familiares.">
<meta name="keywords" content="El Salvador, Centro Terapeutico de Adicciones, Eduado Loyola, Víctor M. Reyes">
<title>{% block title %}{% endblock %}</title>
{% block stylesheets %}
<link rel="shortcut icon" href="{{ asset('images/_favicon.ico') }}" />    
<script src="{{ asset('resources/ctaelsalvador/assets/js/jquery-3.2.1.slim.min.js') }}"></script>
<link rel="stylesheet" href="{{ asset('resources/ctaelsalvador/assets/plugins/bootstrap/css/bootstrap.min.css') }}" type="text/css">
<link rel="stylesheet" href="{{ asset('resources/ctaelsalvador/assets/css/main.css?nocache')}}='{{ random(999) }}'" type="text/css">
</head>
{% endblock %}
<body>
{% block contenido %}{% endblock %}
{% block footer %}
{{ include('footer.html.twig') }}
{% endblock %}
{% block javascripts %}
<script src="{{ asset('resources/ctaelsalvador/assets/js/popper.min.js') }}"></script>
<script rel="stylesheet" src="{{ asset('resources/ctaelsalvador/assets/plugins/bootstrap/js/bootstrap.min.js') }}"></script>
<link rel="stylesheet" href="{{ asset('resources/ctaelsalvador/assets/css/font-awesome.min.css') }}" type="text/css">
{% endblock %}
</body>
{% endspaceless %}